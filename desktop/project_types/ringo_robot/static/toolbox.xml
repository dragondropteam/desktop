            </value>
            <value name="INPUT">
                <shadow type="variables_get">
                    <field name="VAR">defaultValue</field>
                </shadow>
            </value>
        </block>
        <block type="arduino_string_ends_with">
            <value name="STRING">
                <shadow type="variables_get">
                    <field name="VAR">defaultString</field>
                </shadow>
            </value>
            <value name="INPUT">
                <shadow type="variables_get">
                    <field name="VAR">defaultValue</field>
                </shadow>
            </value>
        </block>
        <block type="arduino_string_equals">
            <value name="LHS">
                <shadow type="variables_get">
                    <field name="VAR">stringA</field>
                </shadow>
            </value>
            <value name="RHS">
                <shadow type="variables_get">
                    <field name="VAR">stringB</field>
                </shadow>
            </value>

        </block>
        <block type="arduino_string_index_of">
            <value name="STRING">
                <shadow type="variables_get">
                    <field name="VAR">defaultString</field>
                </shadow>
            </value>
            <value name="INPUT">
                <shadow type="variables_get">
                    <field name="VAR">defaultValue</field>
                </shadow>
            </value>
            <value name="POSITION">
                <shadow type="math_number">
                    <field name="NUM">0</field>
                </shadow>
            </value>
        </block>
        <block type="arduino_string_length">
            <value name="STRING">
                <shadow type="variables_get">
                    <field name="VAR">defaultString</field>
                </shadow>
            </value>
        </block>
        <block type="arduino_string_remove">
            <value name="STRING">
                <shadow type="variables_get">
                    <field name="VAR">defaultString</field>
                </shadow>
            </value>
            <value name="START">
                <shadow type="math_number">
                    <field name="NUM">0</field>
                </shadow>
            </value>
        </block>
        <block type="arduino_string_replace">
            <value name="STRING">
                <shadow type="variables_get">
                    <field name="VAR">defaultString</field>
                </shadow>
            </value>
            <value name="LHS">
                <shadow type="text">
                    <field name="TEXT">defaultText</field>
                </shadow>
            </value>
            <value name="RHS">
                <shadow type="text">
                    <field name="TEXT">defaultText</field>
                </shadow>
            </value>
        </block>
        <block type="arduino_string_starts_with">
            <value name="LHS">
                <shadow type="variables_get">
                    <field name="VAR">stringA</field>
                </shadow>
            </value>
            <value name="RHS">
                <shadow type="variables_get">
                    <field name="VAR">stringB</field>
                </shadow>
            </value>
        </block>
        <block type="arduino_string_substring">
            <value name="STRING">
                <shadow type="variables_get">
                    <field name="VAR">defaultString</field>
                </shadow>
            </value>
            <value name="START">
                <shadow type="math_number">
                    <field name="NUM">0</field>
                </shadow>
            </value>
            <value name="END">
                <shadow type="math_number">
                    <field name="NUM">0</field>
                </shadow>
