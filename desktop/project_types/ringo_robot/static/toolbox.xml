<xml id="toolbox" style="display: none">
    <category name="Logic" colour="210">
        <block type="controls_if"></block>
        <block type="logic_compare"></block>
        <block type="logic_operation"></block>
        <block type="logic_negate"></block>
        <block type="logic_boolean"></block>
        <block type="logic_null"></block>
        <block type="logic_ternary"></block>
    </category>
    <category name="Loops" colour="120">
        <block type="controls_repeat_ext">
            <value name="TIMES">
                <shadow type="math_number">
                    <field name="NUM">10</field>
                </shadow>
            </value>
        </block>
        <block type="controls_whileUntil"></block>
        <block type="controls_for">
            <value name="FROM">
                <shadow type="math_number">
                    <field name="NUM">1</field>
                </shadow>
            </value>
            <value name="TO">
                <shadow type="math_number">
                    <field name="NUM">10</field>
                </shadow>
            </value>
            <value name="BY">
                <shadow type="math_number">
                    <field name="NUM">1</field>
                </shadow>
            </value>
        </block>
        <block type="controls_flow_statements"></block>
    </category>
    <category name="Math" colour="230">
        <block type="math_number" gap="32"></block>
        <block type="math_arithmetic">
            <value name="A">
                <shadow type="math_number">
                    <field name="NUM">1</field>
                </shadow>
            </value>
            <value name="B">
                <shadow type="math_number">
                    <field name="NUM">1</field>
                </shadow>
            </value>
        </block>
        <block type="math_single">
            <value name="NUM">
                <shadow type="math_number">
                    <field name="NUM">9</field>
                </shadow>
            </value>
        </block>
        <block type="math_trig">
            <value name="NUM">
                <shadow type="math_number">
                    <field name="NUM">45</field>
                </shadow>
            </value>
        </block>
        <block type="math_constant"></block>
        <block type="math_change">
            <value name="DELTA">
                <shadow type="math_number">
                    <field name="NUM">1</field>
                </shadow>
            </value>
        </block>
        <block type="math_round">
            <value name="NUM">
                <shadow type="math_number">
                    <field name="NUM">3.1</field>
                </shadow>
            </value>
        </block>
        <block type="math_modulo">
            <value name="DIVIDEND">
                <shadow type="math_number">
                    <field name="NUM">64</field>
                </shadow>
            </value>
            <value name="DIVISOR">
                <shadow type="math_number">
                    <field name="NUM">10</field>
                </shadow>
            </value>
        </block>
        <block type="math_constrain">
            <value name="VALUE">
                <shadow type="math_number">
                    <field name="NUM">50</field>
                </shadow>
            </value>
            <value name="LOW">
                <shadow type="math_number">
                    <field name="NUM">1</field>
                </shadow>
            </value>
            <value name="HIGH">
                <shadow type="math_number">
                    <field name="NUM">100</field>
                </shadow>
            </value>
        </block>
        <block type="math_random_int">
            <value name="FROM">
                <shadow type="math_number">
                    <field name="NUM">1</field>
                </shadow>
            </value>
            <value name="TO">
                <shadow type="math_number">
                    <field name="NUM">100</field>
                </shadow>
            </value>
        </block>
        <block type="arduino_random_seed">
            <value name="SEED">
                <shadow type="analog_read">
                    <value name="PIN">
                        <shadow type="math_number">
                            <field name="NUM">0</field>
                        </shadow>
                    </value>
                </shadow>
            </value>
        </block>
        <block type="arduino_random">
            <value name="MIN">
                <shadow type="math_number">
                    <field name="NUM">0</field>
                </shadow>
            </value>
            <value name="MAX">
                <shadow type="math_number">
                    <field name="NUM">0</field>
                </shadow>
            </value>
        </block>
        <block type="arduino_srand">
            <value name="SEED">
                <shadow type="math_number">
                    <field name="NUM">0</field>
                </shadow>
            </value>
        </block>
    </category>
    <sep></sep>
    <category name="Time" colour="120" enabled="false">
        <block type="delayseconds">
            <value name="SECONDS">
                <shadow type="math_number">
                    <field name="NUM">0</field>
                </shadow>
            </value>
        </block>
        <block type="delayms">
            <value name="MILLISECONDS">
                <shadow type="math_number">
                    <field name="NUM">0</field>
                </shadow>
            </value>
        </block>
        <block type="delaymicroseconds">
            <value name="MICROSECONDS">
                <shadow type="math_number">
                    <field name="NUM">0</field>
                </shadow>
            </value>
        </block>
        <block type="microseconds"></block>
        <block type="milliseconds"></block>
    </category>
    <category name="Digital I/O" colour="120">
        <block type="pininput">
            <value name='PIN'>
                <shadow type="math_number">
                    <field name="NUM">0</field>
                </shadow>
            </value>
        </block>
        <block type="digitalwrite">
            <value name="PIN">
                <shadow type="math_number">
                    <field name="NUM">0</field>
                </shadow>
            </value>
        </block>
        <block type="digital_read">
            <value name="PIN">
                <shadow type="math_number">
                    <field name="NUM">0</field>
                </shadow>
            </value>
        </block>
    </category>
    <category name="Analog I/O" colour="120">
        <block type="analog_reference"></block>
        <block type="analog_read">
            <value name="PIN">
                <shadow type="math_number">
                    <field name="NUM">0</field>
                </shadow>
            </value>
        </block>
        <block type="analog_write">
            <value name="VALUE">
                <shadow type="math_number">
                    <field name="NUM">0</field>
                </shadow>
            </value>
            <value name="PIN">
                <shadow type="math_number">
                    <field name="NUM">0</field>
                </shadow>
            </value>
        </block>
    </category>
    <category name="Advanced I/O" colour="120">
        <block type="tone_pin_freq">
            <value name="FREQUENCY">
                <shadow type="math_number">
                    <field name="NUM">0</field>
                </shadow>
            </value>
            <value name="PIN">
                <shadow type="math_number">
                    <field name="NUM">0</field>
                </shadow>
            </value>
        </block>
        <block type="tone_pin_freq_duration">
            <value name="FREQUENCY">
                <shadow type="math_number">
                    <field name="NUM">0</field>
                </shadow>
            </value>
            <value name="PIN">
                <shadow type="math_number">
                    <field name="NUM">0</field>
                </shadow>
            </value>
            <value name="DURATION">
                <shadow type="math_number">
                    <field name="NUM">0</field>
                </shadow>
            </value>
        </block>
    </category>
    <category name="Constants" colour="120">
        <block type="high"></block>
        <block type="low"></block>
        <block type="led_builtin"></block>
    </category>
    <category name="Serial" colour="120">
        <block type="text"></block>
        <block type="serial_begin"></block>
        <block type="serial_end"></block>
        <block type="serial_print_line"></block>
        <block type="serial_print"></block>
        <block type="serial_read"></block>
    </category>
    <sep></sep>
    <category name="Ringo Hardware" colour="290">
        <category name="Input" colour="290">
            <block type="switch_button_to_pixels"></block>
            <block type="switch_pixels_to_button"></block>
            <block type="switch_motors_to_serial"></block>
            <block type="ringo_button_pressed"></block>
        </category>
        <category name="Pixels" colour="290">
            <block type="set_pixel_rgb">
                <value name="RED">
                    <shadow type="math_number">
                        <field name="NUM">0</field>
                    </shadow>
                </value>
                <value name="GREEN">
                    <shadow type="math_number">
                        <field name="NUM">0</field>
                    </shadow>
                </value>
                <value name="BLUE">
                    <shadow type="math_number">
                        <field name="NUM">0</field>
                    </shadow>
                </value>
            </block>
            <block type="set_all_pixel_rgb">
                <value name="RED">
                    <shadow type="math_number">
                        <field name="NUM">0</field>
                    </shadow>
                </value>
                <value name="GREEN">
                    <shadow type="math_number">
                        <field name="NUM">0</field>
                    </shadow>
                </value>
                <value name="BLUE">
                    <shadow type="math_number">
                        <field name="NUM">0</field>
                    </shadow>
                </value>
            </block>
            <block type="set_pixel_rgb_colour"></block>
            <block type="set_all_pixel_rgb_colour"></block>
        </category>
        <category name="Ringo Motors" colour="290">
            <block type="get_motor_max"></block>
            <block type="get_left_motor"></block>
            <block type="get_right_motor"></block>
            <block type="motors_begin"></block>
            <block type="motors">
                <value name="leftspeed">
                    <shadow type="math_number">
                        <field name="NUM">0</field>
                    </shadow>
                </value>
                <value name="rightspeed">
                    <shadow type="math_number">
                        <field name="NUM">0</field>
                    </shadow>
                </value>
            </block>
        </category>
        <category name="Ringo Movement" colour="290">
            <block type="maintain_heading_reset"></block>
            <block type="maintain_heading">
                <value name="HEADING">
                    <shadow type="math_number">
                        <field name="NUM">0</field>
                    </shadow>
                </value>
                <value name="SPEED">
                    <shadow type="math_number">
                        <field name="NUM">0</field>
                    </shadow>
                </value>
                <value name="WIGGLE">
                    <shadow type="math_number">
                        <field name="NUM">0</field>
                    </shadow>
                </value>
            </block>
            <block type="drive_arc">
                <value name="HEADING">
                    <shadow type="math_number">
                        <field name="NUM">0</field>
                    </shadow>
                </value>
                <value name="LEFT_SPEED">
                    <shadow type="math_number">
                        <field name="NUM">0</field>
                    </shadow>
                </value>
                <value name="RIGHT_SPEED">
                    <shadow type="math_number">
                        <field name="NUM">0</field>
                    </shadow>
                </value>
                <value name="MAX_EXPECTED_TURN_TIME">
                    <shadow type="math_number">
                        <field name="NUM">0</field>
                    </shadow>
                </value>
                <value name="MAX_EXPECTED_SKID_TIME">
                    <shadow type="math_number">
                        <field name="NUM">0</field>
                    </shadow>
                </value>
            </block>
            <block type="rotate_accurate">
                <value name="HEADING">
                    <shadow type="math_number">
                        <field name="NUM">0</field>
                    </shadow>
                </value>
                <value name="MAX_EXPECTED_TURN_TIME">
                    <shadow type="math_number">
                        <field name="NUM">0</field>
                    </shadow>
                </value>
            </block>
            <block type="heading_with_shortest_distance">
                <value name="HEADING">
                    <shadow type="math_number">
                        <field name="NUM">0</field>
                    </shadow>
                </value>
            </block>
            <!--<block type="heading_with_shortest_distance">-->
            <!--<value name="HEADING">-->
            <!--<shadow type="math_number">-->
            <!--<field name="NUM">0</field>-->
            <!--</shadow>-->
            <!--</value>-->
            <!--</block>-->
        </category>
        <category name="Ringo IR Communication" colour="290">
            <block type="restart_ir"></block>
            <block type="is_ir_done"></block>
            <block type="ir_when"></block>
        </category>
        <category name="Ringo Sensors" colour="290">
            <block type="read_side_sensors"></block>
            <block type="read_left_light_sensor"></block>
            <block type="read_right_light_sensor"></block>
            <block type="edge_lights_on"></block>
            <block type="edge_lights_off"></block>
            <block type="reset_look_at_edge"></block>
            <block type="look_at_edge"></block>
            <block type="look_for_edge"></block>
            <block type="read_edge_light_sensors">
                <value name="AVERAGE">
                    <shadow type="math_number">
                        <field name="NUM">0</field>
                    </shadow>
                </value>
            </block>
            <block type="is_over_edge"></block>
            <block type="switch_ambient_to_edge"></block>
            <block type="switch_edge_to_ambient"></block>
        </category>
    </category>
    <category name="Ringo Navigation" colour="290">
        <category name="Navigation" colour="290">
            <block type="get_gyro_calibration_multiplier"></block>
            <block type="begin_navigation"></block>
            <block type="simple_gyro_navigation"></block>
            <block type="simple_navigation"></block>
            <block type="navigation_xy">
                <value name="gyro_sensitivity">
                    <shadow type="math_number">
                        <field name="NUM">50</field>
                    </shadow>
                </value>
                <value name="accel_sensitivity">
                    <shadow type="math_number">
                        <field name="NUM">800</field>
                    </shadow>
                </value>
            </block>
            <block type="calibrate_navigation_sensors"></block>
            <block type="zero_navigation"></block>
            <block type="pause_navigation"></block>
            <block type="resume_navigation"></block>
            <block type="navigation_paused"></block>
            <block type="delay_with_navigation_ms">
                <value name="milliseconds">
                    <shadow type="math_number">
                        <field name="NUM">0</field>
                    </shadow>
                </value>
            </block>
            <block type="delay_with_simple_navigation_ms">
                <value name="milliseconds">
                    <shadow type="math_number">
                        <field name="NUM">0</field>
                    </shadow>
                </value>
            </block>
            <block type="delay_with_navigation_seconds">
                <value name="SECONDS">
                    <shadow type="math_number">
                        <field name="NUM">0</field>
                    </shadow>
                </value>
            </block>
            <block type="delay_with_simple_navigation_seconds">
                <value name="SECONDS">
                    <shadow type="math_number">
                        <field name="NUM">0</field>
                    </shadow>
                </value>
            </block>
            <block type="present_heading"></block>
            <block type="get_degrees_x"></block>
            <block type="get_degrees_per_second_z"></block>
            <block type="get_degrees_per_second_x"></block>
            <block type="get_degrees_per_second_y"></block>
            <block type="get_degrees_to_stop"></block>
            <block type="get_accel_x"></block>
            <block type="get_accel_y"></block>
            <block type="get_accel_y_unzeroed"></block>
            <block type="get_accel_z"></block>
            <block type="get_vel_x"></block>
            <block type="get_vel_y"></block>
            <block type="get_pos_x"></block>
            <block type="get_pos_y"></block>
            <block type="is_stationary"></block>
            <block type="non_stationary_value"></block>
            <block type="non_stationary_axis"></block>
        </category>
        <category name="Calculation" colour="290">
            <block type="min_turn">
                <value name="changeInDegrees">
                    <shadow type="math_number">
                        <field name="NUM">0</field>
                    </shadow>
                </value>
            </block>
            <block type="min_turn_to_heading">
                <value name="desiredHeading">
                    <shadow type="math_number">
                        <field name="NUM">0</field>
                    </shadow>
                </value>
            </block>
            <block type="vector_to_degrees">
                <value name="xComponent">
                    <shadow type="math_number">
                        <field name="NUM">0</field>
                    </shadow>
                </value>
                <value name="yComponent">
                    <shadow type="math_number">
                        <field name="NUM">0</field>
                    </shadow>
                </value>
            </block>
        </category>
    </category>
    <category name="Ringo Fun Stuff" colour="290">
        <category name="Eyes" colour="290">
            <block type="off_pixels"></block>
            <block type="off_pixel"></block>
            <block type="random_eyes"></block>
        </category>
        <category name="Short Animations" colour="290">
            <block type="play_start_chirp"></block>
            <block type="play_ack"></block>
            <block type="play_non_ack"></block>
            <block type="play_anger"></block>
            <block type="play_boredom"></block>
            <block type="play_excited"></block>
        </category>
        <category name="Sound" colour="290">
            <block type="play_chirp">
                <value name="FREQUENCY">
                    <shadow type="math_number">
                        <field name="NUM">0</field>
                    </shadow>
                </value>
                <value name="AMPLITUDE">
                    <shadow type="math_number">
                        <field name="NUM">0</field>
                    </shadow>
                </value>
            </block>
            <block type="off_chirp"></block>
        </category>
    </category>
