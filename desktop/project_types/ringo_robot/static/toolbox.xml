    <category name="String" colour="#bc1106">
        <block type="arduino_string_create">
            <value name="INPUT">
                <shadow type="text">
                    <field name="TEXT">defaultText</field>
                </shadow>
            </value>
        </block>
        <block type="arduino_string_create_base">
            <value name="INPUT">
                <shadow type="math_number">
                    <field name="NUM">0</field>
