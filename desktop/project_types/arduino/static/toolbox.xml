        <block type="arduino_math_map">
            <value name="VALUE">
                <shadow type="math_number">
                    <field name="NUM">0</field>
                </shadow>
            </value>
            <value name="FROM_LOW">
                <shadow type="math_number">
                    <field name="NUM">0</field>
                </shadow>
            </value>
            <value name="FROM_HIGH">
                <shadow type="math_number">
                    <field name="NUM">0</field>
