                <shadow type="math_number">
                    <field name="NUM">0</field>
                </shadow>
            </value>
        </block>
        <block type="delayms">
            <value name="MILLISECONDS">
                <shadow type="math_number">
                    <field name="NUM">0</field>
                </shadow>
            </value>
