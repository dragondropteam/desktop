{
  "name": "DragonDrop",
  "version": "2.0.0-beta.11",
  "author": {
    "name": "DigiPen Institute of Technology",
    "email": "digiblocks@digipen.edu",
    "url": "wwwprod5.digipen.edu"
  },
  "description": "Blockly based visual programming IDE",
  "homepage": "wwprod5.digipen.edu",
  "main": "main.js",
  "build": {
    "directories": {
      "output": "../dist"
    },
    "appId": "edu.digipen.dragondrop",
    "productName": "Dragon Drop",
    "copyright": "Dragon Drop Copyright DigiPen 2016, Blockly Copyright Google, Ace Editor Copyright Cloud9, Golden Layout copyright deepstreamIO",
    "fileAssociations": [
      {
        "ext": "digiblocks",
        "name": "Dragon Drop"
      },
      {
        "ext": "drop",
        "name": "Dragon Drop"
      }
    ],
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "license": "./build/license_en.txt"
    }
  },
  "scripts": {
    "start": "electron main.js",
    "start-dev": "electron main_dev.js",
    "test": "electron-mocha",
    "rebuild": "electron-rebuild .",
    "build": "ALLOW_ELECTRON_BUILDER_AS_PRODUCTION_DEPENDENCY=true gulp"
  },
  "devDependencies": {
    "arduino": "file:project_types/arduino",
    "del": "latest",
    "devtron": "^1.4.0",
    "electron": "^1.7.0 ",
    "electron-rebuild": "^1.6.0",
    "gulp": "^3.9.1",
    "gulp-add-src": "^0.2.0",
    "gulp-clean": "^0.3.2",
    "gulp-uglify": "^3.0.0",
    "gulp-yarn": "^1.0.1",
    "pump": "^1.0.2",
    "standard": "^10.0.2",
    "uglify-es": "^3.2.2",
    "username": "*"
  },
  "dependencies": {
    "angular": "*",
    "angular-animate": "*",
    "angular-aria": "*",
    "angular-material": "^1.1.5",
    "arduino": "file:project_types/arduino",
    "arduino_core": "file:arduino_core",
    "electron-builder": "19.37.2",
    "electron-log": "^2.2.11",
    "electron-store": "*",
    "filesystem": "file:filesystem",
    "fs-extra": "^4.0.3",
    "golden-layout": "^1.5.8",
    "gulp-install": "^1.1.0",
    "is-invalid-path": "*",
    "jszip": "^3.1.5",
    "progress_dialog": "file:progress_dialog",
    "project": "file:project",
    "project_types": "file:project_types",
    "promisepipe": "^2.1.1",
    "rxjs": "^5.5.7",
    "semver": "^5.5.0",
    "serialport": "^6.0.4",
    "username": "^3.0.0",
    "wink_robot": "file:project_types/wink_robot",
    "workspace": "file:workspace",
    "yargs": "^10.0.3",
    "zip-folder": "*"
